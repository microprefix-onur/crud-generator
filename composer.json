{
    "name": "microprefix/crud-generator",
    "description": "A Laravel package for generating API resources from existing models and migrations.",
    "keywords": ["laravel", "api", "resources", "generator"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "microprefix",
            "email": "onur@microprefix.com"
        }
    ],
    "require": {
        "php": "^7.3||^8.0",
        "illuminate/support": ">=8.0",
        "illuminate/routing": ">=8.0",
        "illuminate/console": ">=8.0",
        "doctrine/inflector": ">=2.0",
        "doctrine/dbal": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Microprefix\\CrudGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microprefix\\CrudGenerator\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
